{
    "name": "autocomplete-inline-properties",
    "version": "2.7.0",
    "description": "Add inline property autocompletion for fields that can use inline properties or otherwise reference document data.",
    "private": true,
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "lint": "eslint --ext .js,.mjs .",
        "lint:fix": "eslint --ext .js,.mjs --fix .",
        "format": "prettier --write \"./**/*.(js|mjs|json|css|yml)\"",
        "release": "standard-version",
        "postinstall": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ghost-fvtt/FVTT-Autocomplete-Inline-Properties.git"
    },
    "contributors": [
        {
            "name": "Cole Schultz",
            "discord": "cole#9640"
        },
        {
            "name": "Johannes Loher",
            "email": "johannes.loher@fg4f.de",
            "discord": "ghost#2000"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ghost-fvtt/FVTT-Autocomplete-Inline-Properties/issues"
    },
    "homepage": "https://github.com/ghost-fvtt/FVTT-Autocomplete-Inline-Properties#readme",
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@guanghechen/rollup-plugin-copy": "^1.8.5",
        "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
        "eslint": "^8.4.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "prettier": "^2.5.1",
        "rollup": "^2.60.2",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "standard-version": "^9.3.2",
        "stringify-package": "^1.0.1"
    },
    "lint-staged": {
        "*.(js|mjs)": "eslint --fix",
        "*.(json|css|yml)": "prettier --write"
    }
}
